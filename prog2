#include<stdio.h>
#include<omp.h>

int main(){
	int its,threads;
	printf("Enter the number of threads");
	scanf("%d",&threads);
	printf("Enter the number of iterations");
	scanf("%d",&its);
	omp_set_num_threads(threads);
	#pragma omp parallel for schedule(static,2)
		for(int i=0;i<its;i++){	
			printf("\n Thread %d: iteration %d \n", omp_get_thread_num(),i);
		}
	return 0;
	
}
